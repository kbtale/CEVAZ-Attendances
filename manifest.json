{
    "manifest_version": 3,
    "name": "CEVAZ Attendance Helper",
    "description": "A minimal helper for CEVAZ attendances forms",
    "version": "0.0.1",
    "author": "Carlos Bol√≠var",
    "icons": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png",
        "256": "logo/logo-256.png",
        "512": "logo/logo-512.png"
    },
    "action": {
        "default_title": "CEVAZ Attendance Helper",
        "default_popup": "popup/popup.html"
    },
    "permissions": ["tabs"],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
    "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["*://*.cevaz.org/classroom/mod/attendance/sessions.php*&action=1"],
        "js": ["fill.js"],
        "css": ["select.css"]
    }]
}
